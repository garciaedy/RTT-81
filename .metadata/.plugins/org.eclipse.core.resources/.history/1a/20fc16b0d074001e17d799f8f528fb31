package com.garcia.departmentproject.model;

import jakarta.persistence.Entity;

@Entity

public class Department {
	

	
	private Integer id;
	private String name;
	private String state;
	public Department(Integer id, String name, String state) {
		super();
		this.id = id;
		this.name = name;
		this.state = state;
	}
	
}
